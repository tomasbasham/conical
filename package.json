{
  "name": "conical",
  "version": "0.2.0",
  "description": "A library providing statistical hypothesis testing for front end applications",
  "main": "dist/conical.js",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "babel -d dist src",
    "docs": "yuidoc",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "test": "nyc --reporter=text --reporter=html mocha --compilers js:babel-register"
  },
  "repository": "https://github.com/tomasbasham/conical",
  "engines": {
    "node": ">= 4.3.0"
  },
  "keywords": [
    "statistical",
    "hypothesis",
    "javascript",
    "conical",
    "split",
    "test",
    "ab"
  ],
  "author": "Tomas Basham <me@tomasbasham.co.uk>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.1",
    "mock-local-storage": "^1.0.5",
    "nyc": "^11.3.0",
    "sinon": "^2.1.0",
    "yuidocjs": "^0.10.2"
  }
